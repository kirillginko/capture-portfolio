0vh;\n  padding-top: 20vh;\n  position: relative;\n  h2 {\n    position: absolute;\n    top: 10%;\n    left: 50%;\n    transform: translate(-50%, -10%);\n  }\n  img {\n    width: 100%;\n    height: 70vh;\n    object-fit: cover;\n  }\n`;\nconst Awards = styled.div`\n  min-height: 80vh;\n  display: flex;\n  margin: 5rem 10rem;\n  align-items: center;\n  justify-content: space-around;\n`;\n\nconst AwardStyle = styled.div`\n  padding: 5rem;\n  h3 {\n    font-size: 2rem;\n  }\n  .line {\n    width: 100%;\n    background: #23d997;\n    height: 0.5rem;\n    margin: 1rem 0rem;\n  }\n  p {\n    padding: 2rem 0rem;\n  }\n`;\nconst ImageDisplay = styled.div`\n  min-height: 50vh;\n  img {\n    width: 100%;\n    height: 100vh;\n    object-fit: cover;\n  }\n`;\n\n//Award Component\nconst Award = ({ title, description }) => {\n  return (\n    <AwardStyle>\n      <h3>{title}</h3>\n      <div className=\"line\"></div>\n      <p>{description}</p>\n    </AwardStyle>\n  );\n};\n\nexport default MovieDetail;\n","/Users/kirillginko/Desktop/Kirill Ginko/code/react-apps/capture-portfolio/src/movieState.js",[],"/Users/kirillginko/Desktop/Kirill Ginko/code/react-apps/capture-portfolio/src/animation.js",[],"/Users/kirillginko/Desktop/Kirill Ginko/code/react-apps/capture-portfolio/src/components/Wave.js",[],"/Users/kirillginko/Desktop/Kirill Ginko/code/react-apps/capture-portfolio/src/components/Toggle.js",[],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":13,"column":18,"nodeType":"81","messageId":"82","endLine":13,"endColumn":27},"no-native-reassign",["83"],"no-negated-in-lhs",["84"],"no-unused-vars","'setMovies' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]